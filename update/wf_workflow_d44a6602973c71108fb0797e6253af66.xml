        <value>(function executeRule(current, previous /*null when async*/ ) {&#13;
&#13;
    // Add your code here&#13;
    var attachment = current.variables.file;&#13;
    if (attachment) {&#13;
        var excelParser = new sn_impex.GlideExcelParser();&#13;
        var excelData = excelParser.parse(attachment);&#13;
&#13;
        for (var i = 0; i &lt; excelData.length; i++) {&#13;
            var catalogItem = new GlideRecord('sc_cat_item');&#13;
            catalogItem.initialize();&#13;
            catalogItem.name = excelData[i].Name; // Use the appropriate column name&#13;
            // Set other fields for catalog items&#13;
            var catalogItemId = catalogItem.insert();&#13;
            gs.info("Name of catalog item is :" + catalogItem.name);&#13;
            var variable = new GlideRecord('sc_item_option');&#13;
            variable.initialize();&#13;
            variable.name = excelData[i].Name; // Use the appropriate column name&#13;
            // Set other fields for variables&#13;
            variable.insert();&#13;
            gs.info("Name of variable item is :" + variable.name);&#13;
        }&#13;
    } else {&#13;
        gs.error('No Excel file uploaded.');&#13;
    }&#13;
&#13;
})(current, previous);</value>
